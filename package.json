{
  "name": "we-plugin-acl",
  "description": "Role-based access control (RBAC) for We.js framework",
  "version": "1.2.16",
  "main": "plugin.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js",
    "coverage": "NODE_ENV=test LOG_LV=info nyc ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b"
  },
  "dependencies": {
    "we-core-acl": "^1.2.4"
  },
  "devDependencies": {
    "connect-sqlite3": "^0.9.11",
    "nyc": "^15.0.0",
    "sqlite3": "^4.1.1",
    "fs-extra": "^8.1.0",
    "mocha": "^10.2.0",
    "sinon": "^7.4.2",
    "supertest": "^4.0.2",
    "we-core": "^3",
    "we-plugin-auth": "^2.3.2",
    "we-plugin-user": "^2",
    "we-test-tools": "^1.0.0"
  },
  "keywords": [
    "we.js",
    "plugin",
    "ACL",
    "RBAC",
    "wejs-plugin"
  ],
  "repository": "wejs/we-plugin-acl",
  "author": "Alberto Souza <contato@albertosouza.net>",
  "license": "MIT",
  "wejs": {
    "plugins": {
      "we-plugin-auth": true,
      "we-plugin-user": true
    },
    "dependencies": {
      "we-plugin-auth": true,
      "we-plugin-user": true
    }
  }
}
